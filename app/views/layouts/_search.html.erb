<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag properties_path, :method => 'get' do %>
      <p>
        <%= text_field_tag :search, params[:search],id: 'geo-input' %>
        <%= submit_tag "Search", :location => nil %>
      </p>
    <% end %>
    <%= link_to 'Advanced Search' , new_search_path %>
  </div>
</div>


