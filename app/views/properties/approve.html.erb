<%= form_for(approve_properties_path, method: :put) do %>
  <% @properties.each do |property| %>
    <tr>
      <td class="mdl-data-table__cell--non-numeric"><%= check_box_tag "property_id[]", property.id %></td>
      <td><%= property.price %></td>
    </tr>
  <% end %>
  <%= submit_tag "Mark as approved" %>
<% end %>